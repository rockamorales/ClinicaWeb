	<queryString>
		<![CDATA[SELECT
     tbl_expediente_pacientes.`NUM_EXPEDIENTE` AS tbl_expediente_pacientes_NUM_EXPEDIENTE,
     tbl_expediente_pacientes.`FEC_REG_EXPEDIENTE` AS tbl_expediente_pacientes_FEC_REG_EXPEDIENTE,
